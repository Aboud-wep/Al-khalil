    { "ayah": "قل هو الله احد", "suraName": "الإخلاص", "ayahId": 1 },
    { "ayah": "الله الصمد", "suraName": "الإخلاص", "ayahId": 2 },
    { "ayah": "لم يلد ولم يولد", "suraName": "الإخلاص", "ayahId": 3 },
    { "ayah": "ولم يكن له كفوا احد", "suraName": "الإخلاص", "ayahId": 4 },
    { "ayah": "قل اعوذ برب الفلق", "suraName": "الفلق", "ayahId": 1 },
    { "ayah": "من شر ما خلق", "suraName": "الفلق", "ayahId": 2 },
    { "ayah": "ومن شر غاسق اذا وقب", "suraName": "الفلق", "ayahId": 3 },
    { "ayah": "ومن شر النفاثات في العقد", "suraName": "الفلق", "ayahId": 4 },
    { "ayah": "ومن شر حاسد اذا حسد", "suraName": "الفلق", "ayahId": 5 },
    { "ayah": "قل اعوذ برب الناس", "suraName": "الناس", "ayahId": 1 },
    { "ayah": "ملك الناس", "suraName": "الناس", "ayahId": 2 },
    { "ayah": "اله الناس", "suraName": "الناس", "ayahId": 3 },
    { "ayah": "من شر الوسواس الخناس", "suraName": "الناس", "ayahId": 4 },
    { "ayah": "الذي يوسوس في صدور الناس", "suraName": "الناس", "ayahId": 5 },
