  "ayahs": [
    { "ayah": "انه فكر وقدر", "suraName": "المدثر", "ayahId": 18 },
    { "ayah": "فقتل كيف قدر", "suraName": "المدثر", "ayahId": 19 },
    { "ayah": "ثم قتل كيف قدر", "suraName": "المدثر", "ayahId": 20 },
    { "ayah": "ثم نظر", "suraName": "المدثر", "ayahId": 21 },
    { "ayah": "ثم عبس وبسر", "suraName": "المدثر", "ayahId": 22 },
    { "ayah": "ثم ادبر واستكبر", "suraName": "المدثر", "ayahId": 23 },
    { "ayah": "فقال ان هذا الا سحر يؤثر", "suraName": "المدثر", "ayahId": 24 },
    { "ayah": "ان هذا الا قول البشر", "suraName": "المدثر", "ayahId": 25 },
    { "ayah": "ساصليه سقر", "suraName": "المدثر", "ayahId": 26 },
    { "ayah": "وما ادراك ما سقر", "suraName": "المدثر", "ayahId": 27 },
    { "ayah": "لا تبقي ولا تذر", "suraName": "المدثر", "ayahId": 28 },
    { "ayah": "لواحة للبشر", "suraName": "المدثر", "ayahId": 29 },
    { "ayah": "عليها تسعة عشر", "suraName": "المدثر", "ayahId": 30 },
    {
      "ayah": "وما جعلنا اصحاب النار الا ملائكة وما جعلنا عدتهم الا فتنة للذين كفروا ليستيقن الذين اوتوا الكتاب ويزداد الذين امنوا ايمانا ولا يرتاب الذين اوتوا الكتاب والمؤمنون وليقول الذين في قلوبهم مرض والكافرون ماذا اراد الله بهذا مثلا كذلك يضل الله من يشاء ويهدي من يشاء وما يعلم جنود ربك الا هو وما هي الا ذكرى للبشر",
      "suraName": "المدثر",
      "ayahId": 31
    },
    { "ayah": "كلا والقمر", "suraName": "المدثر", "ayahId": 32 },
